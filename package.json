{
  "name": "rest-api-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "dotenv -e .env.dev docusaurus start",
    "build:internal": "dotenv -e .env.internal docusaurus build",
    "build:enterprise": "dotenv -e .env.enterprise docusaurus build",
    "build:public": "dotenv -e .env.public docusaurus build",
    "prebuild:internal": "yarn lint",
    "prebuild:enterprise": "yarn lint",
    "prebuild:public": "yarn lint",
    "deploy:internal": "dotenv -e .env.internal docusaurus deploy",
    "deploy:enterprise": "dotenv -e .env.enterprise docusaurus deploy",
    "deploy:public": "dotenv -e .env.public docusaurus deploy",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:markdownlint && yarn lint:sidebar",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix && yarn lint:markdownlint:fix",
    "lint:eslint": "yarn eslint . --quiet",
    "lint:eslint:fix": "yarn eslint . --quiet --fix",
    "lint:prettier": "yarn lint:prettier:js && yarn lint:prettier:md",
    "lint:prettier:fix": "yarn lint:prettier:js:fix && yarn lint:prettier:md:fix",
    "lint:prettier:js": "yarn prettier \"**/*.js\" --list-different --config .prettierrc",
    "lint:prettier:js:fix": "yarn prettier \"**/*.js\" --list-different --config .prettierrc --write",
    "lint:prettier:md": "yarn prettier \"**/*.md\" --list-different --config .prettiermarkdownrc",
    "lint:prettier:md:fix": "yarn prettier \"**/*.md\" --list-different --config .prettiermarkdownrc --write",
    "lint:markdownlint": "yarn markdownlint docs/",
    "lint:markdownlint:fix": "yarn markdownlint docs/ --fix",
    "lint:sidebar": "node scripts/lint_sidebars.js",
    "lint:extlinks": "node scripts/lint_external_links.js",
    "lint:endpoints": "node scripts/lint_endpoint.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/preset-classic": "^3.0.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@cmfcmf/docusaurus-search-local": "^1.1.0",
    "@docusaurus/eslint-plugin": "^3.0.1",
    "@docusaurus/module-type-aliases": "^3.0.1",
    "@docusaurus/tsconfig": "^3.0.1",
    "chalk": "5.3.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "markdownlint-cli": "^0.37.0",
    "prettier": "^3.1.0",
    "shelljs": "^0.8.5",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}