{
  "name": "rest-api-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "dotenv -e .env.dev docusaurus start",
    "build:internal": "dotenv -e .env.internal docusaurus build",
    "build:public": "dotenv -e .env.public docusaurus build",
    "prebuild:internal": "yarn lint",
    "prebuild:public": "yarn lint",
    "deploy:internal": "dotenv -e .env.internal docusaurus deploy",
    "deploy:public": "dotenv -e .env.public docusaurus deploy",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:markdownlint",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix && yarn lint:markdownlint:fix",
    "lint:eslint": "yarn eslint . --quiet",
    "lint:eslint:fix": "yarn eslint . --quiet --fix",
    "lint:prettier": "yarn lint:prettier:js && yarn lint:prettier:md",
    "lint:prettier:fix": "yarn lint:prettier:js:fix && yarn lint:prettier:md:fix",
    "lint:prettier:js": "yarn prettier \"**/*.js\" --list-different --config .prettierrc",
    "lint:prettier:js:fix": "yarn prettier \"**/*.js\" --list-different --config .prettierrc --write",
    "lint:prettier:md": "yarn prettier \"**/*.md\" --list-different --config .prettiermarkdownrc",
    "lint:prettier:md:fix": "yarn prettier \"**/*.md\" --list-different --config .prettiermarkdownrc --write",
    "lint:markdownlint": "yarn markdownlint docs/",
    "lint:markdownlint:fix": "yarn markdownlint docs/ --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/eslint-plugin": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "chalk": "^4.1.2",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.7.0",
    "shelljs": "^0.8.5",
    "typescript": "^4.6.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
